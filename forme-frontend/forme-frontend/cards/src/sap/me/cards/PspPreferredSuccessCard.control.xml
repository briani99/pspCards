            <Text class="sapUiSmallMargin sapUiNoMarginBegin" text="Goals:" />
            <Link class="sapUiSmallMargin sapUiNoMarginBegin" text="5" type="Navigation"/>

            <Text class="sapUiSmallMargin sapUiNoMarginBegin" text="Services:" />
            <Link class="sapUiSmallMargin sapUiNoMarginBegin" text="110" type="Navigation"/>

            <Text class="sapUiSmallMargin sapUiNoMarginBegin" text="QBR:" />
            <Link class="sapUiSmallMargin sapUiNoMarginBegin" text="11" type="Navigation"/>

            <Text class="sapUiSmallMargin sapUiNoMarginBegin" text="Learnings:" />
            <Link class="sapUiSmallMargin sapUiNoMarginBegin" text="26" type="Navigation"/>
        </me:headerContentLeft>
